<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <HediffDef ParentName="ImplantHediffBase">
        <defName>ClawHand</defName>
        <label>claw hand</label>
        <labelNoun>a claw hand</labelNoun>
        <spawnThingOnRemoved>ClawHand</spawnThingOnRemoved>
        <addedPartProps>
            <solid>true</solid>
            <partEfficiency>1.5</partEfficiency>
        </addedPartProps>
        <comps>
            <li Class="HediffCompProperties_VerbGiver">
                <tools>
                    <li>
                        <label>claw</label>
                        <capacities>
                            <li>Scratch</li>
                        </capacities>
                        <power>14</power> <!-- Same as natural fist -->
                        <cooldownTime>2</cooldownTime>
                        <alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
                    </li>
                </tools>
            </li>
        </comps>
    </HediffDef>

    <HediffDef ParentName="ImplantHediffBase">
        <defName>JazzlerFinger</defName>
        <label>jazzler Finger</label>
        <labelNoun>a jazzler Finger</labelNoun>
        <spawnThingOnRemoved>JazzlerFinger</spawnThingOnRemoved>
        <addedPartProps>
            <solid>true</solid>
            <partEfficiency>1.5</partEfficiency>
        </addedPartProps>
        <comps>
            <li Class="HediffCompProperties_VerbGiver">
                <tools>
                    <li>
                        <label>claw</label>
                        <capacities>
                            <li>Scratch</li>
                        </capacities>
                        <power>3</power> <!-- Same as natural fist -->
                        <cooldownTime>2</cooldownTime>
                        <alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
                    </li>
                </tools>
            </li>
        </comps>
    </HediffDef>

    <HediffDef ParentName="ImplantHediffBase">
        <defName>Fangs</defName>
        <label>fangs</label>
        <labelNoun>a fangs</labelNoun>
        <spawnThingOnRemoved>Fangs</spawnThingOnRemoved>
        <addedPartProps>
            <solid>true</solid>
            <partEfficiency>1</partEfficiency>
        </addedPartProps>
        <comps>
            <li Class="HediffCompProperties_VerbGiver">
                <tools>
                    <li>
                        <label>teeth</label>
                        <capacities>
                            <li>Bite</li>
                        </capacities>
                        <power>3</power> <!-- Same as natural fist -->
                        <cooldownTime>2</cooldownTime>
                        <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
                        <chanceFactor>0.07</chanceFactor>
                    </li>
                </tools>
            </li>
        </comps>
    </HediffDef>

    <HediffDef ParentName="ImplantHediffBase">
        <defName>CyberSkull</defName>
        <label>cyber skull</label>
        <labelNoun>a cyber skull</labelNoun>
        <spawnThingOnRemoved>CyberSkull</spawnThingOnRemoved>
        <addedPartProps>
            <solid>true</solid>
            <partEfficiency>1.5</partEfficiency>
        </addedPartProps>
        <stages>
            <li>
                <statOffsets>
                    <ArmorRating_Sharp>0.2</ArmorRating_Sharp>
                    <ArmorRating_Blunt>0.2</ArmorRating_Blunt>
                </statOffsets>
            </li>
        </stages>
    </HediffDef>

    <HediffDef ParentName="ImplantHediffBase">
        <defName>TuffBone</defName>
        <label>tuff bone</label>
        <labelNoun>a tuff bone</labelNoun>
        <spawnThingOnRemoved>TuffBone</spawnThingOnRemoved>
        <addedPartProps>
            <solid>true</solid>
            <partEfficiency>1.5</partEfficiency>
        </addedPartProps>
        <stages>
            <li>
                <statOffsets>
                    <ArmorRating_Sharp>0.4</ArmorRating_Sharp>
                    <ArmorRating_Blunt>0.4</ArmorRating_Blunt>
                </statOffsets>
            </li>
        </stages>
    </HediffDef>

    <HediffDef ParentName="ImplantHediffBase">
        <defName>Aj6Implant</defName>
        <label>aj6 implant</label>
        <labelNoun>a aj6 implant</labelNoun>
        <spawnThingOnRemoved>Aj6Implant</spawnThingOnRemoved>
        <stages>
            <li>
                <statOffsets>
                    <GlobalLearningFactor>0.4</GlobalLearningFactor>
                    <MentalBreakThreshold>0.4</MentalBreakThreshold>
                    <TradePriceImprovement>0.4</TradePriceImprovement>
                </statOffsets>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.2</offset>
					</li>
				</capMods>
            </li>
        </stages>
    </HediffDef>

</Defs>