<?xml version="1.0" encoding="utf-8" ?>
<Defs>    

    <ThingDef ParentName="PlantFoodRawBase">
	    <defName>RawWasakaBerries</defName>
	    <label>wasakaberries</label>
	    <description>Red colored berry that grows in bushes. Nice to eat, even when raw.</description>
	    <graphicData>
            <texPath>Things/Item/Resource/PlantFoodRaw/WasakaBerries</texPath>
        </graphicData>
        <statBases>
            <MarketValue>1.5</MarketValue>
	        <Mass>0.027</Mass>
            <FoodPoisonChanceFixedHuman>0.0</FoodPoisonChanceFixedHuman>
        </statBases>
        <comps>
            <li Class="CompProperties_Rottable">
                <daysToRotStart>14</daysToRotStart>
		        <rotDestroys>true</rotDestroys>
            </li>
        </comps>
        <ingestible>
            <preferability>RawTasty</preferability> <!-- So pawns don't eat them for food if other stuff is available -->
	        <tasteThought></tasteThought>
            <foodType>VegetableOrFruit</foodType>
        </ingestible>
    </ThingDef>

</Defs>    